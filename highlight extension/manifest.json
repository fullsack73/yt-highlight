{
  "manifest_version": 3,
  "name": "YouTube Highlight Extractor",
  "version": "1.0",
  "description": "Get current YouTube video URL and display it in a new tab",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": ["tabs", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}